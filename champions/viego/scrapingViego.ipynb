    "\n",
    "def extract_text_without_tags(element):\n",
    "    result = []\n",
    "    for item in element.children:\n",
    "        if isinstance(item, str):\n",
    "            if item != \"\\n\":\n",
    "                result.append(item)\n",
    "        else:\n",
    "            result += html.extract_text_without_tags(item)\n",
    "\n",
    "    return result"
   ]
  },
