   "source": [
    "def get_divs_above_img(img_alt_text):\n",
    "    img_tag = html.find('img', alt=img_alt_text)\n",
    "\n",
    "    if img_tag:\n",
    "        parent_div = img_tag.find_parent('div')\n",
    "        divs_above = parent_div.find_all_previous('div', limit=1)\n",
    "\n",
    "        return divs_above\n",
    "\n",
    "    return None"
   ]
  },
  {
